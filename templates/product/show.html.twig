{% extends 'base.html.twig' %}


{% block body %}
<h1>Product</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Size</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>{{ product.name }}</td>
      <td>
        {{ product.description }}
      </td>
      <td>{{ product.size }}</td>
    </tr>
  </tbody>
</table>



{% endblock %}
